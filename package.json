{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.14.2 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background --clean",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy azle_crud",
        "canister_deploy_mainnet": "dfx deploy --network ic azle_crud",
        "canister_uninstall": "dfx canister uninstall-code azle_crud",
        "canister_call_get_message": "dfx canister call azle_crud getMessage",
        "canister_call_set_message": "dfx canister call azle_crud setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.17.1"
    }
}

